    "eslint-plugin-vue": "^9.17.0",
    "markdown-it-prism": "^2.3.0",
    "prettier": "^3.0.1",
    "sass": "^1.65.1",
    "typescript": "^5.1.6",
    "unocss": "^0.55.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.35",
    "vite-plugin-vue-markdown": "^0.23.7",
    "vue-tsc": "^1.8.8"
