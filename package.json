  "scripts": {
    "dev": "vite",
    "build": "pnpm clean && pnpm typecheck && vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prettier": "prettier --config .prettierrc --write .",
    "lint": "eslint --ext .ts,vue --ignore-pattern '!.*ignore' .",
    "lint:fix": "pnpm lint --fix && pnpm prettier && pnpm lint --fix",
    "clean": "rm -rf ./dist && rm -rf ./node_modules/.vite"
  },
  "dependencies": {
    "@vueuse/core": "^10.3.0",
    "element-plus": "^2.3.9",
    "logrocket": "^5.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.19",
    "@iconify-json/ep": "^1.1.12",
    "@iconify-json/mdi": "^1.1.54",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/eslint-config": "^0.55.0",
    "@unocss/reset": "^0.55.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/runtime-core": "^3.3.4",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.46.0",
