{
  "name": "unocss-study-examples",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.14.0",
  "author": "whidy",
  "scripts": {
    "dev": "VITE_CJS_TRACE=true vite",
    "build": "pnpm clean && pnpm typecheck && vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "prettier": "prettier --config .prettierrc --write .",
    "lint": "eslint --ext .ts,vue --ignore-pattern '!.*ignore' .",
    "lint:fix": "pnpm lint --fix && pnpm prettier && pnpm lint --fix",
    "clean": "rm -rf ./dist && rm -rf ./node_modules/.vite"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "element-plus": "^2.5.1",
    "logrocket": "^7.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.27",
    "@iconify-json/ep": "^1.1.14",
    "@iconify-json/mdi": "^1.1.64",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@unocss/eslint-config": "^0.58.3",
    "@unocss/preset-icons": "^0.58.3",
    "@unocss/reset": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/runtime-core": "^3.4.13",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-vue": "^9.20.1",
    "markdown-it-prism": "^2.3.0",
    "prettier": "^3.2.2",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-markdown": "^0.25.2",
    "vite": "^5.0.11",
    "vite-plugin-inspect": "^0.8.1",
    "vue-tsc": "^1.8.27"
  },
  "license": "MIT"
}
